  "Create Symlink": {
    "prefix": "symlink_banao",
    "body": [
      "symlink_banao(\"${1:target}\", \"${2:linkname}\");"
    ],
    "description": "Create symbolic link (সিমলিংক বানাও)"
  },
  "Read Symlink": {
    "prefix": "symlink_poro",
    "body": [
      "dhoro ${1:target} = symlink_poro(\"${2:linkname}\");",
      "dekho(\"Points to:\", ${1:target});"
    ],
    "description": "Read symlink target (সিমলিংক পড়ো)"
  },
  "Check Symlink": {
    "prefix": "symlink_ki",
    "body": [
      "jodi (symlink_ki(\"${1:path}\")) {",
      "\tdekho(\"${2:Is a symlink}\");",
      "}"
    ],
    "description": "Check if path is symlink (সিমলিংক কি)"
  },
  "Memory Info": {
    "prefix": "mem-info",
    "body": [
      "dhoro ${1:total} = memory_total();",
      "dhoro ${2:used} = memory_bebohrito();",
      "dekho(\"Memory: \", ${2:used}, \"/\", ${1:total}, \"bytes\");"
    ],
    "description": "Get memory information"
  },
  "Disk Space": {
    "prefix": "disk-space",
    "body": [
      "dhoro ${1:total} = disk_akar(\"${2:/}\");",
      "dhoro ${3:free} = disk_mukt(\"${2:/}\");",
      "dekho(\"Disk: \", ${3:free}, \"free of\", ${1:total}, \"bytes\");"
    ],
    "description": "Get disk space information"
  },
  "Network Interfaces": {
    "prefix": "network_interface",
    "body": [
      "dhoro ${1:interfaces} = network_interface();",
      "ghuriye (dhoro i = 0; i < dorghyo(${1:interfaces}); i = i + 1) {",
      "\tdhoro iface = ${1:interfaces}[i];",
      "\tdekho(\"Interface:\", iface[\"naam\"]);",
      "}"
    ],
    "description": "Get network interfaces (নেটওয়ার্ক ইন্টারফেস)"
  },
  "IP Addresses": {
    "prefix": "ip_shokal",
    "body": [
      "dhoro ${1:ips} = ip_shokal();",
      "dekho(\"All IP addresses:\", ${1:ips});"
    ],
    "description": "Get all IP addresses (আইপি সকল)"
  },
  "File Search Pattern": {
    "prefix": "file-search",
    "body": [
      "// Find all .txt files in directory",
      "dhoro ${1:allFiles} = directory_ghumao(\"${2:.}\");",
      "ghuriye (dhoro i = 0; i < dorghyo(${1:allFiles}); i = i + 1) {",
      "\tdhoro file = ${1:allFiles}[i];",
      "\tjodi (file_ext(file) == \".${3:txt}\") {",
      "\t\tdekho(\"Found:\", file);",
      "\t}",
      "}"
    ],
    "description": "Search for files by pattern"
  },
  "System Monitor": {
    "prefix": "sys-monitor",
    "body": [
      "kaj monitor() {",
      "\tdhoro mem_total = memory_total();",
      "\tdhoro mem_used = memory_bebohrito();",
      "\tdhoro disk_free = disk_mukt(\"/\");",
      "\t",
      "\tdekho(\"Memory:\", mem_used, \"/\", mem_total);",
      "\tdekho(\"Disk free:\", disk_free);",
      "\t",
      "\tprocess_ghum(${1:5000});",
      "\tmonitor(); // Recursive call",
      "}",
      "",
      "monitor();"
    ],
    "description": "System resource monitor"
  },
  "Backup Script": {
    "prefix": "backup-script",
    "body": [
      "// Simple backup script",
      "dhoro ${1:source} = \"${2:/path/to/source}\";",
      "dhoro ${3:backup} = \"${4:/path/to/backup}\";",
      "",
      "jodi (!ache_ki(${3:backup})) {",
      "\tfolder_banao_shokal(${3:backup});",
      "}",
      "",
      "dhoro files = directory_ghumao(${1:source});",
      "dekho(\"Backing up\", dorghyo(files), \"files...\");",
      "",
      "// Copy files (using chalan for cp command)",
      "dhoro result = chalan(\"cp\", [\"-r\", ${1:source}, ${3:backup}]);",
      "jodi (result[\"code\"] == 0) {",
      "\tdekho(\"Backup completed successfully!\");",
      "} nahole {",
      "\tdekho(\"Backup failed:\", result[\"error\"]);",
      "}"
    ],
    "description": "File backup script template"
  },
  "PostgreSQL Connection": {
