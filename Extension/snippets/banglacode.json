{
  "Variable Declaration": {
    "prefix": "dhoro",
    "body": [
      "dhoro ${1:name} = ${2:value};"
    ],
    "description": "Declare a variable (ধরো)"
  },
  "Constant Declaration": {
    "prefix": "sthir",
    "body": [
      "sthir ${1:NAME} = ${2:value};"
    ],
    "description": "Declare a constant - cannot be changed (স্থির)"
  },
  "Global Variable": {
    "prefix": "bishwo",
    "body": [
      "bishwo ${1:name} = ${2:value};"
    ],
    "description": "Declare a global variable (বিশ্ব)"
  },
  "If Statement": {
    "prefix": "jodi",
    "body": [
      "jodi (${1:condition}) {",
      "\t${2:// code}",
      "}"
    ],
    "description": "If condition (যদি)"
  },
  "If-Else Statement": {
    "prefix": "jodi-nahole",
    "body": [
      "jodi (${1:condition}) {",
      "\t${2:// if true}",
      "} nahole {",
      "\t${3:// if false}",
      "}"
    ],
    "description": "If-else condition (যদি-নাহলে)"
  },
  "While Loop": {
    "prefix": "jotokkhon",
    "body": [
      "jotokkhon (${1:condition}) {",
      "\t${2:// code}",
      "}"
    ],
    "description": "While loop (যতক্ষণ)"
  },
  "For Loop": {
    "prefix": "ghuriye",
    "body": [
      "ghuriye (dhoro ${1:i} = ${2:0}; ${1:i} < ${3:10}; ${1:i} = ${1:i} + 1) {",
      "\t${4:// code}",
      "}"
    ],
    "description": "For loop (ঘুরিয়ে)"
  },
  "Function": {
    "prefix": "kaj",
    "body": [
      "kaj ${1:name}(${2:params}) {",
      "\t${3:// code}",
      "\tferao ${4:result};",
      "}"
    ],
    "description": "Function definition (কাজ)"
  },
  "Function No Return": {
    "prefix": "kaj-void",
    "body": [
      "kaj ${1:name}(${2:params}) {",
      "\t${3:// code}",
      "}"
    ],
    "description": "Function without return"
  },
  "Function with Rest Parameter": {
    "prefix": "kaj-rest",
    "body": [
      "kaj ${1:name}(...${2:args}) {",
      "\tghuriye (dhoro i = 0; i < dorghyo(${2:args}); i = i + 1) {",
      "\t\t${3:// process each argument}",
      "\t}",
      "}"
    ],
    "description": "Function with rest parameter (...args)"
  },
  "Variadic Sum Function": {
    "prefix": "kaj-sum",
    "body": [
      "kaj ${1:sum}(...${2:numbers}) {",
      "\tdhoro total = 0;",
      "\tghuriye (dhoro i = 0; i < dorghyo(${2:numbers}); i = i + 1) {",
      "\t\ttotal = total + ${2:numbers}[i];",
      "\t}",
      "\tferao total;",
      "}"
    ],
    "description": "Variadic sum function using rest parameter"
  },
  "Spread Array": {
    "prefix": "spread",
    "body": [
      "...${1:array}"
    ],
    "description": "Spread operator to expand array"
  },
  "Combine Arrays with Spread": {
    "prefix": "spread-combine",
    "body": [
      "dhoro ${1:combined} = [...${2:arr1}, ...${3:arr2}];"
    ],
    "description": "Combine arrays using spread operator"
  },
  "Class": {
    "prefix": "sreni",
    "body": [
      "sreni ${1:ClassName} {",
      "\tshuru(${2:params}) {",
      "\t\tei.${3:property} = ${3:property};",
      "\t}",
      "\t",
      "\tkaj ${4:method}() {",
      "\t\t${5:// code}",
      "\t}",
      "}"
    ],
    "description": "Class definition (শ্রেণী)"
  },
  "New Instance": {
    "prefix": "notun",
    "body": [
      "dhoro ${1:instance} = notun ${2:ClassName}(${3:args});"
    ],
    "description": "Create new instance (নতুন)"
  },
  "Print": {
    "prefix": "dekho",
    "body": [
      "dekho(${1:value});"
    ],
    "description": "Print output (দেখো)"
  },
  "Print Variable": {
    "prefix": "dekhov",
    "body": [
      "dekho(\"${1:label}:\", ${2:variable});"
    ],
    "description": "Print labeled variable"
  },
  "Try-Catch": {
    "prefix": "chesta",
    "body": [
      "chesta {",
      "\t${1:// try code}",
      "} dhoro_bhul (${2:err}) {",
      "\tdekho(\"Error:\", ${2:err});",
      "}"
    ],
    "description": "Try-catch block (চেষ্টা)"
  },
  "Try-Catch-Finally": {
    "prefix": "chesta-shesh",
    "body": [
      "chesta {",
      "\t${1:// try code}",
      "} dhoro_bhul (${2:err}) {",
      "\tdekho(\"Error:\", ${2:err});",
      "} shesh {",
      "\t${3:// finally code}",
      "}"
    ],
    "description": "Try-catch-finally block (চেষ্টা-শেষ)"
  },
  "Throw Error": {
    "prefix": "felo",
    "body": [
      "felo \"${1:error message}\";"
    ],
    "description": "Throw error (ফেলো)"
  },
  "Import Module": {
    "prefix": "ano",
    "body": [
      "ano \"${1:module.bang}\";"
    ],
    "description": "Import module (আনো)"
  },
  "Import Module with Alias": {
    "prefix": "ano-hisabe",
    "body": [
      "ano \"${1:module.bang}\" hisabe ${2:alias};"
    ],
    "description": "Import module with alias (আনো হিসাবে)"
  },
  "Export Function": {
    "prefix": "pathao-kaj",
    "body": [
      "pathao kaj ${1:name}(${2:params}) {",
      "\t${3:// code}",
      "\tferao ${4:result};",
      "}"
    ],
    "description": "Export function (পাঠাও)"
  },
  "Export Class": {
    "prefix": "pathao-sreni",
    "body": [
      "pathao sreni ${1:ClassName} {",
      "\tshuru(${2:params}) {",
      "\t\tei.${3:property} = ${3:property};",
      "\t}",
      "}"
    ],
    "description": "Export class (পাঠাও শ্রেণী)"
  },
  "Array": {
    "prefix": "array",
    "body": [
      "dhoro ${1:arr} = [${2:items}];"
    ],
    "description": "Create array (তালিকা)"
  },
  "Map/Object": {
    "prefix": "map",
    "body": [
      "dhoro ${1:obj} = {",
      "\t\"${2:key}\": ${3:value}",
      "};"
    ],
    "description": "Create map/object (মানচিত্র)"
  },
  "Array Iterate": {
    "prefix": "array-ghuri",
    "body": [
      "ghuriye (dhoro ${1:i} = 0; ${1:i} < dorghyo(${2:arr}); ${1:i} = ${1:i} + 1) {",
      "\tdhoro ${3:item} = ${2:arr}[${1:i}];",
      "\t${4:// code}",
      "}"
    ],
    "description": "Iterate over array"
  },
  "Map Iterate": {
    "prefix": "map-ghuri",
    "body": [
      "dhoro ${1:keys} = chabi(${2:obj});",
      "ghuriye (dhoro ${3:i} = 0; ${3:i} < dorghyo(${1:keys}); ${3:i} = ${3:i} + 1) {",
      "\tdhoro ${4:key} = ${1:keys}[${3:i}];",
      "\tdhoro ${5:value} = ${2:obj}[${4:key}];",
      "\t${6:// code}",
      "}"
    ],
    "description": "Iterate over map keys"
  },
  "HTTP Server": {
    "prefix": "server",
    "body": [
      "kaj handleRequest(req, res) {",
      "\tdekho(\"Request:\", req[\"method\"], req[\"path\"]);",
      "\t",
      "\tjodi (req[\"path\"] == \"/\") {",
      "\t\tuttor(res, \"${1:Hello from BanglaCode!}\");",
      "\t} nahole {",
      "\t\tuttor(res, \"Not Found\", 404);",
      "\t}",
      "}",
      "",
      "server_chalu(${2:3000}, handleRequest);"
    ],
    "description": "HTTP server (সার্ভার চালু)"
  },
  "HTTP JSON Response": {
    "prefix": "json-uttor",
    "body": [
      "json_uttor(res, ${1:data});"
    ],
    "description": "Send JSON response (JSON উত্তর)"
  },
  "HTTP Simple Response": {
    "prefix": "uttor",
    "body": [
      "uttor(res, \"${1:message}\");"
    ],
    "description": "Send simple response (উত্তর)"
  },
  "JSON Parse": {
    "prefix": "json_poro",
    "body": [
      "dhoro ${1:data} = json_poro(${2:jsonString});"
    ],
    "description": "Parse JSON string (JSON পড়ো)"
  },
  "JSON Stringify": {
    "prefix": "json_banao",
    "body": [
      "dhoro ${1:jsonStr} = json_banao(${2:object});"
    ],
    "description": "Convert to JSON string (JSON বানাও)"
  },
  "HTTP API Server": {
    "prefix": "api-server",
    "body": [
      "kaj handleAPI(req, res) {",
      "\tdekho(\"Request:\", req[\"method\"], req[\"path\"]);",
      "\t",
      "\tjodi (req[\"path\"] == \"/\") {",
      "\t\tuttor(res, \"${1:BanglaCode API Server}\");",
      "\t} nahole jodi (req[\"path\"] == \"/api/data\") {",
      "\t\tdhoro data = {",
      "\t\t\t\"message\": \"Namaskar!\",",
      "\t\t\t\"status\": \"success\"",
      "\t\t};",
      "\t\tjson_uttor(res, data);",
      "\t} nahole {",
      "\t\tjson_uttor(res, {\"error\": \"Not Found\"}, 404);",
      "\t}",
      "}",
      "",
      "server_chalu(${2:3000}, handleAPI);"
    ],
    "description": "HTTP API server with JSON responses"
  },
  "HTTP GET": {
    "prefix": "anun",
    "body": [
      "dhoro ${1:response} = anun(\"${2:url}\");",
      "dekho(\"Status:\", ${1:response}[\"status\"]);",
      "dekho(\"Body:\", ${1:response}[\"body\"]);"
    ],
    "description": "HTTP GET request (আনুন)"
  },
  "TCP Server": {
    "prefix": "tcp-server",
    "body": [
      "tcp_server_chalu(${1:8080}, kaj(conn) {",
      "\tdekho(\"Client connected:\", conn[\"remote_addr\"]);",
      "\tdekho(\"Data:\", conn[\"data\"]);",
      "\ttcp_pathao(conn, \"${2:Echo: }\" + conn[\"data\"]);",
      "});"
    ],
    "description": "TCP server (TCP সার্ভার চালু)"
  },
  "TCP Client": {
    "prefix": "tcp-client",
    "body": [
      "proyash kaj ${1:tcpClient}() {",
      "\tdhoro conn = opekha tcp_jukto(\"${2:localhost}\", ${3:8080});",
      "\ttcp_lekho(conn, \"${4:Hello TCP!}\");",
      "\tdhoro response = opekha tcp_shuno(conn);",
      "\tdekho(\"Response:\", response);",
      "\ttcp_bondho(conn);",
      "}",
      "${1:tcpClient}();"
    ],
    "description": "TCP client with async/await"
  },
  "UDP Server": {
    "prefix": "udp-server",
    "body": [
      "udp_server_chalu(${1:9000}, kaj(packet) {",
      "\tdekho(\"Received:\", packet[\"data\"]);",
      "\tdekho(\"From:\", packet[\"remote_addr\"]);",
      "\tudp_uttor(packet, \"${2:Response message}\");",
      "});"
    ],
    "description": "UDP server (UDP সার্ভার চালু)"
  },
  "UDP Client": {
    "prefix": "udp-client",
    "body": [
      "proyash kaj ${1:udpClient}() {",
      "\topekha udp_pathao(\"${2:localhost}\", ${3:9000}, \"${4:Hello UDP!}\");",
      "\tdekho(\"Packet sent!\");",
      "}",
      "${1:udpClient}();"
    ],
    "description": "UDP client with async"
  },
  "WebSocket Server": {
    "prefix": "websocket-server",
    "body": [
      "websocket_server_chalu(${1:3000}, kaj(conn) {",
      "\tdekho(\"Client connected:\", conn[\"remote_addr\"]);",
      "\tjodi (conn[\"connected\"] == sotti) {",
      "\t\tdekho(\"Message:\", conn[\"message\"]);",
      "\t\twebsocket_pathao(conn, \"${2:Echo: }\" + conn[\"message\"]);",
      "\t}",
      "});"
    ],
    "description": "WebSocket server (WebSocket সার্ভার চালু)"
  },
  "WebSocket Client": {
    "prefix": "websocket-client",
    "body": [
      "proyash kaj ${1:wsClient}() {",
      "\tdhoro ws = opekha websocket_jukto(\"${2:ws://localhost:3000}\");",
      "\twebsocket_pathao(ws, \"${3:Hello WebSocket!}\");",
      "\tghumaao(${4:1000});",
      "\twebsocket_bondho(ws);",
      "}",
      "${1:wsClient}();"
    ],
    "description": "WebSocket client with async/await"
  },
  "Read File": {
    "prefix": "poro",
    "body": [
      "dhoro ${1:content} = poro(\"${2:filename}\");"
    ],
    "description": "Read file (পড়ো)"
  },
  "Write File": {
    "prefix": "lekho",
    "body": [
      "lekho(\"${1:filename}\", ${2:content});"
    ],
    "description": "Write file (লেখো)"
  },
  "User Input": {
    "prefix": "nao",
    "body": [
      "dhoro ${1:input} = nao(\"${2:Enter value: }\");"
    ],
    "description": "Get user input (নাও)"
  },
  "Sleep": {
    "prefix": "ghum",
    "body": [
      "ghum(${1:1000});"
    ],
    "description": "Sleep milliseconds (ঘুম)"
  },
  "Current Time": {
    "prefix": "somoy",
    "body": [
      "dhoro ${1:time} = somoy();"
    ],
    "description": "Get current timestamp (সময়)"
  },
  "Math Square Root": {
    "prefix": "borgomul",
    "body": [
      "borgomul(${1:number})"
    ],
    "description": "Square root (বর্গমূল)"
  },
  "Math Power": {
    "prefix": "ghat",
    "body": [
      "ghat(${1:base}, ${2:exp})"
    ],
    "description": "Power (ঘাত)"
  },
  "Random Number": {
    "prefix": "lotto",
    "body": [
      "lotto()"
    ],
    "description": "Random 0-1 (লটো)"
  },
  "Constructor Method": {
    "prefix": "shuru",
    "body": [
      "shuru(${1:params}) {",
      "\tei.${2:property} = ${2:property};",
      "}"
    ],
    "description": "Constructor method (শুরু)"
  },
  "Main Template": {
    "prefix": "main",
    "body": [
      "// BanglaCode Program",
      "// Created by: ${1:Ankan}",
      "",
      "dekho(\"${2:Hello, West Bengal!}\");",
      "",
      "kaj main() {",
      "\t${3:// Your code here}",
      "}",
      "",
      "main();"
    ],
    "description": "Main program template"
  },
  "Documentation Comment": {
    "prefix": "@comment",
    "body": [
      "// @comment ${1:Description of function/class}"
    ],
    "description": "Add documentation comment (shown on hover)"
  },
  "Documented Function": {
    "prefix": "kaj-doc",
    "body": [
      "// @comment ${1:What this function does}",
      "// @comment Example: ${2:functionName}(${3:args})",
      "kaj ${2:functionName}(${4:params}) {",
      "\t${5:// code}",
      "\tferao ${6:result};",
      "}"
    ],
    "description": "Function with documentation"
  },
  "Documented Class": {
    "prefix": "sreni-doc",
    "body": [
      "// @comment ${1:Description of this class}",
      "// @comment Use: dhoro obj = notun ${2:ClassName}(${3:args});",
      "sreni ${2:ClassName} {",
      "\tshuru(${3:params}) {",
      "\t\tei.${4:property} = ${4:property};",
      "\t}",
      "\t",
      "\t// @comment ${5:Method description}",
      "\tkaj ${6:methodName}() {",
      "\t\t${7:// code}",
      "\t}",
      "}"
    ],
    "description": "Class with documentation"
  },
  "File Size": {
    "prefix": "file_akar",
    "body": [
      "dhoro ${1:size} = file_akar(\"${2:path/to/file}\");",
      "dekho(\"File size:\", ${1:size}, \"bytes\");"
    ],
    "description": "Get file size in bytes (ফাইল আকার)"
  },
  "File Permissions": {
    "prefix": "file_permission",
    "body": [
      "dhoro ${1:perms} = file_permission(\"${2:path/to/file}\");",
      "dekho(\"Permissions:\", ${1:perms});"
    ],
    "description": "Get file permissions (ফাইল পারমিশন)"
  },
  "Set File Permissions": {
    "prefix": "file_permission_set",
    "body": [
      "file_permission_set(\"${1:path/to/file}\", \"${2:0644}\");"
    ],
    "description": "Set file permissions (ফাইল পারমিশন সেট)"
  },
  "File Metadata": {
    "prefix": "file-metadata",
    "body": [
      "dhoro path = \"${1:path/to/file}\";",
      "dekho(\"Size:\", file_akar(path), \"bytes\");",
      "dekho(\"Permissions:\", file_permission(path));",
      "dekho(\"Type:\", file_dhoron(path));",
      "dekho(\"Modified:\", file_shomoy_poribortito(path));"
    ],
    "description": "Get comprehensive file metadata"
  },
  "File Rename": {
    "prefix": "file_rename",
    "body": [
      "file_rename(\"${1:oldpath}\", \"${2:newpath}\");"
    ],
    "description": "Rename or move file (ফাইল রিনেম)"
  },
  "Directory List": {
    "prefix": "directory_taliika",
    "body": [
      "dhoro ${1:files} = directory_taliika(\"${2:path/to/dir}\");",
      "ghuriye (dhoro i = 0; i < dorghyo(${1:files}); i = i + 1) {",
      "\tdekho(${1:files}[i]);",
      "}"
    ],
    "description": "List directory contents (ডিরেক্টরি তালিকা)"
  },
  "Directory Traverse": {
    "prefix": "directory_ghumao",
    "body": [
      "dhoro ${1:allFiles} = directory_ghumao(\"${2:path/to/dir}\");",
      "dekho(\"Total files:\", dorghyo(${1:allFiles}));"
    ],
    "description": "Recursive directory traversal (ডিরেক্টরি ঘুরাও)"
  },
  "Directory Size": {
    "prefix": "directory_akar",
    "body": [
      "dhoro ${1:totalSize} = directory_akar(\"${2:path/to/dir}\");",
      "dekho(\"Directory size:\", ${1:totalSize}, \"bytes\");"
    ],
    "description": "Get total directory size (ডিরেক্টরি আকার)"
  },
  "Check File Exists": {
    "prefix": "ache_ki",
    "body": [
      "jodi (ache_ki(\"${1:path}\")) {",
      "\tdekho(\"${2:File exists}\");",
      "} nahole {",
      "\tdekho(\"${3:File not found}\");",
      "}"
    ],
    "description": "Check if file exists (আছে কি)"
  },
  "Create Directory": {
    "prefix": "folder_banao",
    "body": [
      "folder_banao(\"${1:path/to/dir}\");"
    ],
    "description": "Create directory (ফোল্ডার বানাও)"
  },
  "Create Nested Directory": {
    "prefix": "folder_banao_shokal",
    "body": [
      "folder_banao_shokal(\"${1:path/to/nested/dir}\");"
    ],
    "description": "Create directory with parents (ফোল্ডার বানাও সকল)"
  },
  "Remove File": {
    "prefix": "muke_felo",
    "body": [
      "muke_felo(\"${1:path}\");"
    ],
    "description": "Remove file or directory (মুছে ফেলো)"
  },
  "Process Sleep": {
    "prefix": "process_ghum",
    "body": [
      "process_ghum(${1:1000}); // Sleep ${1:1000}ms"
    ],
    "description": "Sleep for milliseconds (প্রসেস ঘুম)"
  },
  "Execute Command": {
    "prefix": "chalan",
    "body": [
      "dhoro ${1:result} = chalan(\"${2:command}\", [${3:args}]);",
      "dekho(\"Output:\", ${1:result}[\"output\"]);",
      "dekho(\"Exit code:\", ${1:result}[\"code\"]);"
    ],
    "description": "Execute system command (চালান)"
  },
  "Execute Simple Command": {
    "prefix": "chalan-simple",
    "body": [
      "dhoro ${1:result} = chalan(\"${2:ls}\");",
      "dekho(${1:result}[\"output\"]);"
    ],
    "description": "Execute simple command (চালান)"
  },
  "Start Background Process": {
    "prefix": "process_chalu",
    "body": [
      "dhoro ${1:proc} = process_chalu(\"${2:command}\", [${3:args}]);",
      "dekho(\"Started process:\", ${1:proc}[\"pid\"]);"
    ],
    "description": "Start process in background (প্রসেস চালু)"
  },
  "Kill Process": {
    "prefix": "process_maro",
    "body": [
      "process_maro(${1:pid});"
    ],
    "description": "Kill process by PID (প্রসেস মারো)"
  },
  "Get Process ID": {
    "prefix": "process_id",
    "body": [
      "dhoro ${1:pid} = process_id();",
      "dekho(\"Current PID:\", ${1:pid});"
    ],
    "description": "Get current process ID (প্রসেস আইডি)"
  },
  "Environment Variable": {
    "prefix": "poribesh",
    "body": [
      "dhoro ${1:value} = poribesh(\"${2:VAR_NAME}\");",
      "dekho(\"${2:VAR_NAME}:\", ${1:value});"
    ],
    "description": "Get environment variable (পরিবেশ)"
  },
  "Set Environment Variable": {
    "prefix": "poribesh_set",
    "body": [
      "poribesh_set(\"${1:VAR_NAME}\", \"${2:value}\");"
    ],
    "description": "Set environment variable (পরিবেশ সেট)"
  },
  "All Environment Variables": {
