    "description": "Redis sorted set for leaderboards/rankings"
  },
  "Redis Sets (Tags/Collections)": {
    "prefix": "db-redis-sadd",
    "body": [
      "// Add members to set",
      "db_sadd_redis(${1:conn}, \"${2:tags}\", [\"${3:tag1}\", \"${4:tag2}\", \"${5:tag3}\"]);",
      "",
      "// Get all members",
      "dhoro ${6:members} = db_smembers_redis(${1:conn}, \"${2:tags}\");",
      "",
      "// Set operations",
      "dhoro ${7:common} = db_sinter_redis(${1:conn}, [\"${8:set1}\", \"${9:set2}\"]);",
      "$0"
    ],
    "description": "Redis sets for unique collections"
  },
  "Redis Atomic Counter": {
    "prefix": "db-redis-incr",
    "body": [
      "// Atomic increment (thread-safe)",
      "db_incr_redis(${1:conn}, \"${2:page:views}\");",
      "db_incrby_redis(${1:conn}, \"${3:user:points}\", ${4:50});",
      "",
      "// Get counter value",
      "dhoro ${5:views} = db_get_redis(${1:conn}, \"${2:page:views}\");",
      "$0"
    ],
    "description": "Redis atomic counters for metrics"
  },
  "MongoDB Find with Options": {
    "prefix": "db-mongodb-find-options",
    "body": [
      "dhoro ${1:result} = db_khojo_options_mongodb(${2:conn}, \"${3:collection}\", ",
